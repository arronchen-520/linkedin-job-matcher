{
   "action": {
      "default_popup": "popup.html",
      "default_title": "SpeedyApply: Job Application Autofill Tool"
   },
   "author": "kirk@speedyapply.com",
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content-scripts/content.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_idle"
   } ],
   "description": "Applying to jobs just got easier. Automate job applications with a single click and track your progress over time.",
   "homepage_url": "https://speedyapply.com",
   "host_permissions": [ "*://*/*" ],
   "icons": {
      "128": "icon/128.png",
      "16": "icon/16.png",
      "32": "icon/32.png",
      "48": "icon/48.png",
      "96": "icon/96.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "SpeedyApply: Job Application Autofill Tool",
   "options_ui": {
      "open_in_tab": true,
      "page": "options.html"
   },
   "permissions": [ "storage", "unlimitedStorage", "tabs", "contextMenus", "sidePanel" ],
   "short_name": "SpeedyApply",
   "side_panel": {
      "default_path": "sidepanel.html"
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.19.6",
   "web_accessible_resources": [ {
      "extension_ids": [ "mbgjopdedgonlbpikjfibkccpmhjbnag" ],
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "options.html" ]
   }, {
      "matches": [ "*://*/*" ],
      "resources": [ "content-scripts/content.css" ]
   } ]
}
